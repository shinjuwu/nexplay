{
  "name": "dcc-vue-front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "manager-dev": "vite --config vite.config.manager.js",
    "agent-dev": "vite --config vite.config.agent.js",
    "build-manager-dev": "vite build --mode dev --config vite.config.manager.js --emptyOutDir",
    "build-agent-dev": "vite build --mode dev --config vite.config.agent.js --emptyOutDir",
    "build-dev": "run-p build-manager-dev build-agent-dev",
    "build-manager-qc": "vite build --mode qc --config vite.config.manager.js --emptyOutDir",
    "build-agent-qc": "vite build --mode qc --config vite.config.agent.js --emptyOutDir",
    "build-qc": "run-p build-manager-qc build-agent-qc",
    "build-manager-qa": "vite build --mode qa --config vite.config.manager.js --emptyOutDir",
    "build-agent-qa": "vite build --mode qa --config vite.config.agent.js --emptyOutDir",
    "build-qa": "run-p build-manager-qa build-agent-qa",
    "build": "run-p build-manager-dev build-agent-dev",
    "preview-manager": "vite preview --config vite.config.manager.js",
    "preview-agent": "vite preview --config vite.config.agent.js",
    "lint": "eslint . --ext .js,.vue --fix --ignore-path .gitignore",
    "format": "prettier --write . --ignore-path .gitignore",
    "prepare": "cd ../../ && husky install dcc_front/frontend/.husky"
  },
  "dependencies": {
    "@heroicons/vue": "^2.0.13",
    "axios": "^0.27.2",
    "date-fns": "^2.29.3",
    "echarts": "^5.4.0",
    "lodash": "^4.17.21",
    "pinia": "^2.0.17",
    "vue": "^3.2.37",
    "vue-dragscroll": "^4.0.4",
    "vue-i18n": "^9.14.5",
    "vue-router": "^4.1.3",
    "vuedraggable": "^4.1.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.19.0/xlsx-0.19.0.tgz"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^5.0.1",
    "@rushstack/eslint-patch": "^1.3.2",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tailwindcss": "^3.8.0",
    "eslint-plugin-vue": "^9.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.1",
    "reset-css": "^5.0.1",
    "sass": "^1.97.3",
    "tailwindcss": "^3.2.4",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^3.0.4",
    "vue-eslint-parser": "^9.1.0"
  }
}
